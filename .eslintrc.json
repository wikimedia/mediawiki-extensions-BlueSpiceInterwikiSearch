{
	"root": true,
	"extends": [
		"wikimedia/client-es5",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"blueSpice": "readonly",
		"jQuery": "readonly",
		"mediaWiki": "readonly"
	},
	"rules": {
		"no-jquery/no-global-selector": "off",
		"no-prototype-builtins": "off",
		"no-shadow-restricted-names": "off",
		"no-unused-vars": "off",
		"one-var": "off",
		"vars-on-top": "off"
	}
}
